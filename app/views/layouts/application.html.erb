<!DOCTYPE html>
<html>
  
<head>
  <title><%= title %></title>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= render 'layouts/css' %>
</head>


<body>
  <div class="container">
    
    <%= render 'layouts/header' %>
    
    <section class="round">
      <%= yield %>
    </section>
    
    <%= render 'layouts/footer' %>
    <%= debug(params) if Rails.env.development? %>
    
  </div>
</body>

</html>
